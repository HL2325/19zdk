/* pages/success/success.wxss */
.success{
  width: 100%;
  min-height: 100vh;
  background:linear-gradient(180deg,rgba(255,255,255,1) 0%,rgba(239,241,245,0.74) 100%);
}
.success_img{
  width: 354rpx;
  height: 306rpx;
  margin:  180rpx auto 40rpx;
}
.success_img image{
  width: 100%;
  height: 100%;
}
.success_text{
  color: #1F314A;
  line-height: 80rpx;
  font-size: 60rpx;
  text-align: center;
  margin-bottom: 80rpx;
}
.back_home{
  width: 520rpx;
  height:108rpx;
  background: rgba(102,101,255,1);
  border-radius: 200rpx;
  margin: 0 auto;
  color: #fff;
  text-align: center;
  line-height: 108rpx;
  font-size: 38rpx;
}