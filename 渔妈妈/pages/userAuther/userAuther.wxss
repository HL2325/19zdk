/* pages/userAuther/userAuther.wxss */

.userAuther {
  min-height: 100vh;
  background: linear-gradient(180deg, rgba(255, 255, 255, 1) 0%, rgba(239, 241, 245, 0.74) 100%);
}

.order_title {
  color: #1f314a;
  font-size: 60rpx;
  line-height: 80rpx;
  padding: 60rpx 40rpx 0;
  margin-bottom: 40rpx;
}

.author_main {
  width: 100%;
  height: 910rpx;
  background: url('data:image/png;base64,') no-repeat;
  background-size: 100% 100%;
  border: 1px solid rgba(0,0,0,0);
}

.child_name{
  width:580rpx;
  height:128rpx;
  background:rgba(255,255,255,1);
  box-shadow:0px 12px 12px 0px rgba(52,84,122,0.06);
  border-radius:100px;
  border:1px solid rgba(31,49,74,0.11);
  margin: 170rpx auto 40rpx;
  padding: 0 50rpx;
  color: #1F314A;
}
.child_name input{
  height: 100%;
}
.birth_time{
  width:580rpx;
  height:128rpx;
  background:rgba(255,255,255,1);
  box-shadow:0px 12px 12px 0px rgba(52,84,122,0.06);
  border-radius:100px;
  border:1px solid rgba(31,49,74,0.11);
  margin: 0 auto;
  padding: 0 50rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #ccc;
}
.calendar{
  width: 52rpx;
  height: 52rpx;
}
.calendar image{
  width: 100%;
  height: 100%;
}
.wux-cell__ft{
  color: #1F314A !important;
}
.wux-cell--hover{
  background: none !important;
}
.wux-cell{
  padding: 30rpx 0 !important;
  border: none !important;
}
.wux-cell:after{
  border: none !important;
}
.add_apply{
  width:520rpx;
  height:108rpx;
  background:rgba(102,101,255,1);
  border-radius:100px;
  text-align: center;
  line-height: 108rpx;
  color: #fff;
  font-size: 38rpx;
  margin: 120rpx auto 0;
}