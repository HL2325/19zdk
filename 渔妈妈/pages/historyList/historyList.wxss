/* pages/historyList/historyList.wxss */
.historyList{
  padding: 0 40rpx;
  min-height: 100vh;
  background:linear-gradient(360deg,rgba(255,255,255,1) 0%,rgba(239,241,245,0.74) 100%);
}
.order_title{
  color: #1F314A;
  font-size: 60rpx;
  line-height: 80rpx;
  padding-top: 60rpx;
  margin-bottom: 40rpx;
}
.card{
  background: #fff;
  box-shadow:0px 2px 4px 0px rgba(31,49,74,0.12);
  border-radius:10rpx;
  padding: 20rpx;
  font-size: 30rpx;
}
.teacher{
  background:rgba(243,245,248,1);
  border-radius: 5px;
  padding: 30rpx;
  min-height: 200rpx;
}
.teacher_inside{
  margin-top: 20rpx;
  background:rgba(243,245,248,1);
  border-radius: 5px;
  padding: 30rpx;
  min-height: 200rpx;
}
.card_time{
  font-size: 30rpx;
  color: #1F314A;
  margin-bottom: 10rpx;
}
.noData_img{
  width: 240rpx;
  height: 240rpx;
  margin: 100rpx auto 10rpx;
}
.noData_img image{
  width: 100%;
  height: 100%;
}
.nodata_text{
  font-size: 30rpx;
  color:rgba(25,31,37,0.28);
  text-align: center;
}