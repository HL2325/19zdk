/* pages/phone/phone.wxss */
.register{
  width: 100%;
  min-height: 100vh;
  background:linear-gradient(360deg,rgba(255,255,255,1) 0%,rgba(239,241,245,0.74) 100%);
  overflow: hidden;
}
.register_logo{
  width: 200rpx;
  height: 200rpx;
  margin: 100rpx auto 0;
}
.register_logo image{
  width: 100%;
  height: 100%;
}
.register_main{
  margin-top: 140rpx;
}
.reg_phone{
  width: 600rpx;
  height: 128rpx;
  background:rgba(255,255,255,1);
  box-shadow:0px 12px 12px 0px rgba(52,84,122,0.06);
  border-radius:100px;
  margin: 0 auto;
  display: flex;
  align-items: center;
  padding: 0 44rpx;
  margin-bottom: 30rpx;
}
.reg_phone input{
  height: 100%;
}
.icons{
  width: 44rpx;
  height: 44rpx;
  margin-right: 24rpx;
}
.icons image{
  width: 100%;
  height: 100%;
}
.get_code{
  color: #6665FF;
  font-size: 32rpx;
  background: none;
  border: none;
}
.get_code::after {
  border: none;
}
.author_btn{
  width: 600rpx;
  height: 108rpx;
  background:rgba(102,101,255,1);
  border-radius:100px;
  margin: 84rpx auto 0;
  font-size: 38rpx;
  color: #fff;
  text-align: center;
  line-height: 108rpx;
}