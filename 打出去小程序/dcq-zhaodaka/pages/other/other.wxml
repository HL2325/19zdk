<!--pages/other/other.wxml-->
<view class="other">
  <view class="first_main">
    <view class="other_main">
      <view class="big_title">打出去</view>
      <view class="title_desc">请告诉我们一些个人信息 </view>
      <view class="title_desc">以便更好的为你推荐资源 </view>
      <view class="other_info">
        <view class="other_list">
          <view class="list_img">
            <image src="../../images/xingming.png"></image>
          </view>
          <view>
            <input placeholder-style="color: #8F98A8;" placeholder="请输入您的姓名"></input>
          </view>
        </view>
        <view class="other_list">
          <view class="list_img">
            <image src="../../images/chusheng.png"></image>
          </view>
          <view>
            <input placeholder-style="color: #8F98A8;" placeholder="请输入您的姓名"></input>
          </view>
        </view>
        <view class="other_list">
          <view class="list_img">
            <image src="../../images/jianzhong.png"></image>
          </view>
          <view>
            <input placeholder-style="color: #8F98A8;" placeholder="剑种"></input>
          </view>
        </view>
        <view class="other_list">
          <view class="list_img">
            <image src="../../images/guoji.png"></image>
          </view>
          <view>
            <input placeholder-style="color: #8F98A8;" placeholder="国籍"></input>
          </view>
        </view>
        <view class="other_list">
          <view class="list_img">
            <image src="../../images/shenfen.png"></image>
          </view>
          <view>
            <input placeholder-style="color: #8F98A8;" placeholder="我的打出去身份"></input>
          </view>
        </view>
      </view>
    </view>
    <view class="agreement" bindtap="choAgree">
      <view class="agreement_radio">
        <view class="agreement_true" wx:if="{{agree == true}}"></view>
      </view>
      <view class="agreement_text">
        <text>同意</text>
        <text style="color: #3E72E0;">《用户授权协议》</text>
      </view>
    </view>
    <view class="submit_btn" bindtap="submitBtn">确定</view>
  </view>
</view>