/* pages/honey/honey.wxss */
.honey_main{
  padding: 20rpx;
  box-shadow:0px 5px 9px 0px rgba(0,0,0,0.05);
}
.honey_list{
  padding: 30rpx 20rpx;
  border-bottom: 2rpx solid #f0f0f0;
  font-size: 30rpx;
  color: #333;
  display: flex;
  justify-content: space-between;
}

.no_data{
  width: 100%;
  margin-top: 220rpx;
}
.nd_pic{
  width: 240rpx;
  height: 240rpx;
  margin: 0 auto 10rpx;
}
.nd_pic image{
  width: 100%;
  height: 100%;
}
.nd_text{
  font-size: 30rpx;
  color:rgba(25,31,37,0.28);
  text-align: center;
}
