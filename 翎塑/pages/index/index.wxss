/**index.wxss**/
.clear{
  clear: both;
}
.home{
  width: 100%;
  min-height: 100vh;
  background: #f6f6f6;
  position: relative;
}
.top_main{
  width: 100%;
  height: 300rpx;
  background: #333;
  position: relative;
}
.user{
  width: 700rpx;
  /* height: 320rpx; */
  background: #fff;
  position: absolute;
  top: 40rpx;
  left: calc(50% - 350rpx);
  border-radius: 10rpx;
  padding-bottom: 36rpx;
  border-bottom: 14rpx solid #333;
  box-shadow: #fff 0px 0px 4rpx 4rpx;
}
.avatar{
  position: absolute;
  left: 40rpx;
  top: -20rpx;
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  overflow: hidden;
}
.avatar image {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.user_info{
  margin-left: 210rpx;
  width: 450rpx;
  margin-top: 10rpx;
}
.user_login{
  margin-top: 38rpx;
  margin-left: 210rpx;
}
.user_name{
  font-size: 34rpx;
}
.user_vip{
  display: flex;
  justify-content: flex-start;
}
.vip_icon{
  width: 50rpx;
  height: 50rpx;
}
.vip_icon image{
  width: 100%;
  height: 100%;
}
.vip_text{
  font-size: 26rpx;
  margin: 4rpx 0 0 8rpx;
}
.money_main{
  margin-top: 60rpx;
  display: flex;
  justify-content: space-between;
  text-align: center;
  font-size: 26rpx;
  color: #999;
}
.money_list{
  width: 50%;
}
.commission_num{
  font-size: 34rpx;
  color: #333;
  margin-bottom: 5rpx;
  font-weight: 600;
}
.discount{
  font-size: 28rpx;
  width: 600rpx;
  margin: 100rpx auto 0;
  border: 4rpx solid #333;
  display: flex;
  line-height: 50rpx;
  justify-content: space-between;
  padding: 14rpx 20rpx;
  border-radius: 6rpx;
}
.right_icon{
  width: 50rpx;
  height: 50rpx;
}
.right_icon image{
  width: 100%;
  height: 100%;
}
.order_bot{
  z-index: 1111;
  border-top: 1px solid #333;
  width: 100%;
  height: 100rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background: #333;
  font-size: 24rpx;
  color: #fff;
  display: flex;
  justify-content: space-between;
}
.order_tel{
  position: absolute;
  width: 110rpx;
  height: 110rpx;
  background: #333;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  top: -30rpx;
  left: calc(50% - 60rpx);
  box-shadow: #fff 0px 0px 4rpx 4rpx;
}
.order_tel image{
  width: 80rpx;
  height: 80rpx;
}

.author_user{
  font-size: 30rpx;
  color: #fff;
  background: #333;
  width: 400rpx;
  border-radius: 40rpx;
  margin: 20rpx auto;
}
.info_text{
  font-size: 26rpx;
  line-height: 40rpx;
}
.author_img{
  margin-top: 20rpx;
  width: 529rpx;
  height: 455rpx;
  /* background: #ccc; */
}
.author_img image{
  width: 100%;
  height: 100%;
}
.login_btn{
  background: none;
  width: 200rpx;
  height: 70rpx;
  float: left;
  font-size: 28rpx;
  border-radius: 40rpx;
  border: 1px solid #333;
  color: #333;
  line-height: 70rpx;
}
.copper_color{
  color: #c18065;
}
.silver_color{
  color: #98989a;
}
.gold_color{
  color: #ccb778;
}
.withdrawText{
  color: #333;
  padding: 20rpx 0;
  font-size: 32rpx;
}
.with_label{
  border-bottom: 1rpx solid #ccc;
  padding: 20rpx 0;
}
.with_btn{
  margin-top: 20rpx;
  border-radius: 40rpx;
  color: #fff;
  background: #333;
  font-size: 30rpx;
  width: 260rpx;
  line-height: 80rpx;
  margin: 40rpx auto;
}
.list_card{
  width: 33.33%;
  color: #333;
  font-size: 26rpx;
  margin-bottom: 60rpx;
}
.list_icon{
  width: 80rpx;
  height: 80rpx;
  background: #333;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-right: 50rpx;
  margin: 0 auto 10rpx;
}
.list_icon image{
  width: 50rpx;
  height: 50rpx;
}
.lis_main{
  margin-top: 60rpx;
  padding: 0 40rpx;
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
  text-align: center;
}
.jingx_png{
  width: 50rpx;
  height: 50rpx;
  margin: 10rpx auto 5rpx;
}
.jingx_png image{
  width: 100%;
  height: 100%;
}
.bot_card{
  width: 33.33%;
  text-align: center;
}
.bot_img{
  width: 100%;
  position: absolute;
  top: 100vh;
  padding-bottom: 130rpx;
}
.bot_img image{
  width: 100%;
}